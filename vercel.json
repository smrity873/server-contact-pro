{
    "version": 2,
    "builds": [
        {
            "src": "**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "**/*.ts",
            "use": "@vercel/node"
        },
        {
            "src": "next.config.js",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}